{
  "name": "@jsweb/params",
  "version": "3.1.6",
  "description": "Simple JS module to parse/serialize HTTP query/params, useful for Fetch API or AJAX requests",
  "scripts": {
    "test": "run-s test:*",
    "test:lint": "npm run lint",
    "test:clean": "shx rm -rf mocha*",
    "test:report": "mocha src/**/*.test.ts --require ts-node/register --reporter mochawesome --reporter-options reportTitle=@jsweb/params",
    "major": "npm version major && npm run push",
    "minor": "npm version minor && npm run push",
    "patch": "npm version patch && npm run push",
    "prepare": "npm run dist",
    "push": "git push && git push --tags",
    "lint": "tsc --noEmit && tslint -p . -c tslint.json --fix",
    "dist": "run-s dist:*",
    "dist:test": "npm t",
    "dist:clean": "shx rm -rf dist",
    "dist:types": "tsc --removeComments --emitDeclarationOnly",
    "dist:build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/params.git"
  },
  "keywords": [
    "parse",
    "serialize",
    "query",
    "params",
    "form",
    "data",
    "FormData",
    "http",
    "js",
    "javascript"
  ],
  "author": "Alex Bruno <git.alexbr@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsweb/params/issues"
  },
  "homepage": "https://params.jsweb.app",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^12.0.8",
    "husky": "^2.4.1",
    "mocha": "^6.1.4",
    "mochawesome": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.0",
    "rollup-plugin-esmin": "^0.1.3",
    "rollup-plugin-typescript": "^1.0.0",
    "shx": "^0.3.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.9.3",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add ."
    }
  },
  "main": "dist/umd.js",
  "esnext": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/module.d.ts"
}
