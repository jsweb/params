{
  "name": "@jsweb/params",
  "version": "3.0.4",
  "description": "Simple JS module to parse/serialize HTTP query/params, useful for Fetch API or AJAX requests",
  "main": "dist/umd.js",
  "esnext": "dist/module.js",
  "module": "dist/module.js",
  "types": "dist/module.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsweb/params.git"
  },
  "keywords": [
    "parse",
    "serialize",
    "query",
    "params",
    "form",
    "data",
    "FormData",
    "http",
    "js",
    "javascript"
  ],
  "author": "Alex Bruno <git.alexbr@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsweb/params/issues"
  },
  "homepage": "https://params.jsweb.app",
  "scripts": {
    "dist": "run-s dist:*",
    "dist:clean": "rimraf dist",
    "dist:test": "npm t",
    "dist:types": "tsc --removeComments --emitDeclarationOnly",
    "dist:build": "rollup -c",
    "test": "run-s test:*",
    "test:lint": "tslint -p . -c tslint.json",
    "test:clean": "rimraf mocha*",
    "test:report": "mocha src/**/*.test.ts --require ts-node/register --reporter mochawesome --reporter-options reportTitle=@jsweb/params",
    "prepare": "npm run dist"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "babel-minify": "^0.4.3",
    "concat": "^1.0.3",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.3",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-esmin": "^0.1.3",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  }
}
